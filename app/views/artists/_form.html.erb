<%= form_with(model: artist) do |form| %>

  <div class="form-group">
    <%= form.label :name %><br>
    <%= form.text_field :name, id: :artist_name %>
  </div>
  <div class="form-group">
    <%= form.label :year %><br>
    <%= form.number_field :year, id: :year %>
  </div>

  <div class="actions">
    <%= form.submit "Create", class: "btn btn-primary" %>
  </div>
<% end %>
